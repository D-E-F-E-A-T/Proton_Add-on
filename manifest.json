{
    "name": "Proton Troll",
    "version": "1.0",
    "description": "just for fun and some people doesn't say \"fairplay\"",
    "manifest_version": 2,
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_icon": "img/protonlogo.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "img/protonlogo.png",
        "32": "img/protonlogo.png",
        "48": "img/protonlogo.png",
        "128": "img/protonlogo.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}